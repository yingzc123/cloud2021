<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yzc.springcloud.dao.AdminMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yzc.springcloud.entity.Admin">
        <id column="admin_id" property="adminId" />
        <result column="admin_name" property="adminName" />
        <result column="admin_age" property="adminAge" />
        <result column="admin_address" property="adminAddress" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        admin_id, admin_name, admin_age, admin_address
    </sql>

</mapper>
